# Cache and compiled files
.phpactor.json
.phpunit.result.cache
/.phpunit.cache
/bootstrap/cache/*
!bootstrap/cache/.gitkeep

# IDE and editors
/.fleet
/.idea
/.nova
/.vscode
/.zed
*.swp
*.swo
*~

